{
    "buildCommand": "cd frontend && npm run build",
    "installCommand": "cd frontend && npm install",
    "functions": {
      "api/*.py": {
        "runtime": "@vercel/python@5.0.0",
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    },
    "rewrites": [
      {
        "source": "/api",
        "destination": "/api/index"
      },
      {
        "source": "/api/(.*)",
        "destination": "/api/index/$1"
      }
  ]
}