{
    "buildCommand": "cd frontend && npm run build",
    "installCommand": "cd frontend && npm install",
    "functions": {
      "api/*.py": {
        "runtime": "@vercel/python@5.0.0",
        "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
      }
    },
      "builds": [
        { "src": "frontend/package.json", "use": "@vercel/next@latest" },
        { "src": "api/**/*.py", "use": "@vercel/python@5.0.0" }
      ],
      "routes": [
        { "src": "/api(?:/.*)?", "dest": "/api/index.py" },
        { "src": "/(.*)", "dest": "frontend/$1" }
      ]
    }